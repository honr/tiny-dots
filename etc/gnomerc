# -*- Mode: Shell-Script -*-
# export SYSTYPE="$(uname)"
export SYSOSTYPE="linux"
export SYSARCH="linux-$(uname -m)"

export PATH="$HOME/.local/bin/$SYSARCH:$HOME/.local/bin:$HOME/.opt/bin:$PATH"
export MANPATH="$HOME/.local/share/man:$HOME/.opt/share/man:$MANPATH"
export PYTHONSTARTUP="$HOME/.local/etc/pythonrc"
export ALTERNATE_EDITOR=""
export VISUAL="emacsclient"
export EDITOR="emacsclient"
export MY_BASHRC_LOADED="t"
